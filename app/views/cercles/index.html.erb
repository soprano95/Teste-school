<p id="notice"><%= notice %></p>

<h1>Cercles</h1>

<table>
  <thead>
    <tr>
      <th>Regions</th>
      <th>Cercle name</th>
      <th>Cercle name short</th>
      <th>Position</th>
      <th>Code</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cercles.each do |cercle| %>
      <tr>
        <td><%= cercle.regions %></td>
        <td><%= cercle.cercle_name %></td>
        <td><%= cercle.cercle_name_short %></td>
        <td><%= cercle.position %></td>
        <td><%= cercle.code %></td>
        <td><%= cercle.latitude %></td>
        <td><%= cercle.longitude %></td>
        <td><%= link_to 'Show', cercle %></td>
        <td><%= link_to 'Edit', edit_cercle_path(cercle) %></td>
        <td><%= link_to 'Destroy', cercle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cercle', new_cercle_path %>
