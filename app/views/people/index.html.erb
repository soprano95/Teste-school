<p id="notice"><%= notice %></p>

<h1>People</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Middle name</th>
      <th>Sexe</th>
      <th>Date nai</th>
      <th>Lieu nai</th>
      <th>Situation family</th>
      <th>Regime matrimonial</th>
      <th>Contact</th>
      <th>Email</th>
      <th>Address</th>
      <th>Personid</th>
      <th>Life</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @people.each do |person| %>
      <tr>
        <td><%= person.first_name %></td>
        <td><%= person.last_name %></td>
        <td><%= person.middle_name %></td>
        <td><%= person.sexe %></td>
        <td><%= person.date_nai %></td>
        <td><%= person.lieu_nai %></td>
        <td><%= person.situation_family %></td>
        <td><%= person.regime_matrimonial %></td>
        <td><%= person.contact %></td>
        <td><%= person.email %></td>
        <td><%= person.address %></td>
        <td><%= person.personId %></td>
        <td><%= person.life %></td>
        <td><%= link_to 'Show', person %></td>
        <td><%= link_to 'Edit', edit_person_path(person) %></td>
        <td><%= link_to 'Destroy', person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Person', new_person_path %>
