<p id="notice"><%= notice %></p>

<h1>Schools</h1>

<table>
  <thead>
    <tr>
      <th>Quartier</th>
      <th>School name</th>
      <th>Contacts</th>
      <th>Address</th>
      <th>Academie</th>
      <th>Reinscription</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schools.each do |school| %>
      <tr>
        <td><%= school.quartier %></td>
        <td><%= school.school_name %></td>
        <td><%= school.contacts %></td>
        <td><%= school.address %></td>
        <td><%= school.academie %></td>
        <td><%= school.reinscription %></td>
        <td><%= school.latitude %></td>
        <td><%= school.longitude %></td>
        <td><%= link_to 'Show', school %></td>
        <td><%= link_to 'Edit', edit_school_path(school) %></td>
        <td><%= link_to 'Destroy', school, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New School', new_school_path %>
