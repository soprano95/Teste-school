<p id="notice"><%= notice %></p>

<h1>Communes</h1>

<table>
  <thead>
    <tr>
      <th>Cercle</th>
      <th>Commune name</th>
      <th>Commune name short</th>
      <th>Position</th>
      <th>Code</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @communes.each do |commune| %>
      <tr>
        <td><%= commune.cercle %></td>
        <td><%= commune.commune_name %></td>
        <td><%= commune.commune_name_short %></td>
        <td><%= commune.position %></td>
        <td><%= commune.code %></td>
        <td><%= commune.latitude %></td>
        <td><%= commune.longitude %></td>
        <td><%= link_to 'Show', commune %></td>
        <td><%= link_to 'Edit', edit_commune_path(commune) %></td>
        <td><%= link_to 'Destroy', commune, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Commune', new_commune_path %>
